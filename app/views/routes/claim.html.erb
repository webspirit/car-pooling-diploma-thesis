<h1>Claimed Route</h1>

<table>
  <tr>
    <th>Departure address</th>
    <th>Arrival address</th>
    <th>Distance departure range</th>
    <th>Distance arrival range</th>
    <th>Start time</th>
    <th>Time range from</th>
    <th>Time range to</th>
    <th>User</th>
    <th>Claim Route</th>
  </tr>

<% @routes.each do |route| %>
  <tr>
    <td><%= route.departure_address %></td>
    <td><%= route.arrival_address %></td>
    <td><%= route.distance_departure_range %></td>
    <td><%= route.distance_arrival_range %></td>
    <td><%= route.start_time %></td>
    <td><%= route.time_range_from %></td>
    <td><%= route.time_range_to %></td>
    <td><%= route.user_id %></td>
    <td><%= link_to 'Claim', claim_route_path(@route) %></td>
  </tr>
<% end %>
</table>

<br />